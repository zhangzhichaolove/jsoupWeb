<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.chao.jsoup.model">
    <class name="RequestCount" table="request_count">

        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="tableName" column="tableName"/>
        <property name="lastCount" column="lastCount"/>
        <property name="dataCount" column="dataCount"/>
        <property name="lastUpdateTime" column="lastUpdateTime"/>
    </class>
</hibernate-mapping>